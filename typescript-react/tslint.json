{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
      "interface-name": [
          true,
          "never-prefix"
      ],
      "member-ordering": [
        false
      ],
      "quotemark": [
        true, "single", "jsx-double"
      ],
      "jsx-no-lambda": false,
      "max-line-length": [
        false
      ],
      "curly": false,
      "jsx-self-close": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "trailing-comma": [true, {"multiline": "never", "singleline": "never"}]
  }
}